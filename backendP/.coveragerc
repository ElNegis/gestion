[run]
source = clientes_ventas_cotizaciones
omit =
    */migrations/*
    */tests/*
    */__init__.py
    */admin.py
    */apps.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError

[html]
directory = coverage_html_report